{
  "name": "Capture",
  "version": "0.1.0",
  "description": "Application to download videos",
  "main": "main.js",
  "scripts": {
    "lint": "eslint app *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "license": "MIT",
  "author": "",
  "keywords": [
    "electron",
    "react",
    "redux",
    "music",
    "video",
    "webpack",
    "react-hot"
  ],
  "devDependencies": {
    "asar": "^0.9.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chromedriver": "^2.19.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.0.2",
    "del": "^2.0.1",
    "devtron": "^1.2.0",
    "electron-devtools-installer": "^1.1.5",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.2",
    "electron-rebuild": "^1.0.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.2.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "fbjs-scripts": "^0.5.0",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-libs-browser": ">= 0.4.0 <=0.6.0",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-modules-values": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "redux-logger": "^2.3.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "electron-debug": "^0.5.1",
    "history": "^1.17.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-debounced": "^0.1.1",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.2",
    "source-map-support": "^0.4.2",
    "youtube-dl": "^1.10.5"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
